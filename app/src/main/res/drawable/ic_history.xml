<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24"
    android:tint="@color/text_on_primary">

    <!-- Clock circle -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="@android:color/white"
        android:strokeWidth="1.5"
        android:pathData="M12,4 A8,8 0 1,1 4,12 A8,8 0 0,1 12,4 Z" />

    <!-- Clock hands -->
    <path
        android:fillColor="@android:color/white"
        android:pathData="M12,7 L12,12 L15,12"
        android:strokeColor="@android:color/black"
        android:strokeWidth="1.5"
        android:strokeLineCap="round" />

    <!-- Document lines representing transaction list -->
    <path
        android:fillColor="@android:color/white"
        android:pathData="M3,16 L9,16 M3,18 L7,18 M3,20 L8,20"
        android:strokeColor="@android:color/white"
        android:strokeWidth="1.2"
        android:strokeLineCap="round" />

    <!-- Small arrow indicating movement/flow -->
    <path
        android:fillColor="@android:color/white"
        android:pathData="M18,15 L20,17 L18,19 M16,17 L20,17"
        android:strokeColor="@android:color/white"
        android:strokeWidth="1.2"
        android:strokeLineCap="round"
        android:strokeLineJoin="round" />

</vector>